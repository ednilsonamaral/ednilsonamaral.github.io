@keyframes titleAnimation
    0%
        opacity 0
        translateY(10%)
    100%
        opacity 1
        translateY(0)

@-webkit-keyframes titleAnimation
    0%
        opacity 0
        translateY(10%)
    100%
        opacity 1
        translateY(0)

@keyframes menuHomeAnimation
    0%, 10%
        opacity 0
    20%, 100%
        opacity 1

@-webkit-keyframes menuHomeAnimation
    0%, 10%
        opacity 0
    20%, 100%
        opacity 1
